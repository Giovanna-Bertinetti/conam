<shared-spinner *ngIf="!loaded"></shared-spinner>
<div class="col-xs-12 col-sm-12 col-md-12 col-lg-12" *ngIf="loaded">
    <shared-alert [message]="messageTop" [type]="typeMessageTop" *ngIf="showMessageTop"></shared-alert>
    <div class="form-group row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <h4 class="border-title"><strong>Dati Notifica</strong></h4>

            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div class="form-group col-xs-12 col-sm-12  col-md-6 col-lg-6">
                    <div class="col-xs-12 col-sm-12 col-md-5 col-lg-5">
                        <label class="control-label" for="dataNotifica">Data di Notifica
                        </label>
                    </div>
                    <div class="col-xs-12 col-sm-12 col-md-7 col-lg-7">
                        {{notifica.dataNotifica}}
                    </div>
                </div>
                <div class="form-group col-xs-12 col-sm-12 col-md-6 col-lg-6">
                    <div class="col-xs-12 col-sm-12 col-md-5 col-lg-5">
                        <label class="control-label" for="dataSpedizione">Data spedizione
                        </label> </div>
                    <div class="col-xs-12 col-sm-12 col-md-7 col-lg-7">
                        {{notifica.dataSpedizione}}
                    </div>
                </div>
            </div>



            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div class="form-group col-xs-12 col-sm-12  col-md-6 col-lg-6">
                    <div class="col-xs-12 col-sm-12 col-md-5 col-lg-5">
                        <label class="control-label">Numero Raccomandata</label>
                    </div>
                    <div class="col-xs-12 col-sm-12 col-md-7 col-lg-7">
                        {{notifica.numeroRaccomandata}}
                    </div>
                </div>
                <div class="form-group col-xs-12 col-sm-12 col-md-6 col-lg-6">
                    <div class="col-xs-12 col-sm-12 col-md-5 col-lg-5">
                        <label class="control-label">Importo spese Notifica</label>
                    </div>
                    <div class="col-xs-12 col-sm-12 col-md-7 col-lg-7">
                        {{notifica.importoSpeseNotifica | currency:'EUR':'symbol'}}
                    </div>
                </div>
            </div>
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12" *ngIf="notifica?.modalita?.denominazione != null">
                <div class="form-group col-xs-12 col-sm-12  col-md-6 col-lg-6">
                    <div class="col-xs-12 col-sm-12 col-md-5 col-lg-5">
                        <label class="control-label">Modalit√†</label>
                    </div>
                    <div class="col-xs-12 col-sm-12 col-md-7 col-lg-7">
                        {{notifica?.modalita?.denominazione}}
                    </div>
                </div>
                <div class="form-group col-xs-12 col-sm-12 col-md-6 col-lg-6">
                    <div class="col-xs-12 col-sm-12 col-md-5 col-lg-5">

                    </div>
                    <div class="col-xs-12 col-sm-12 col-md-7 col-lg-7">

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>