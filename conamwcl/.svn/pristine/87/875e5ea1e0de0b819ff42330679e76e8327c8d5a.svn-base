<div class="container-fluid">
    <div class="principal-div col-xs-12 col-sm-12 col-md-1 col-lg-1"></div>
    <div class="principal-div col-xs-12 col-sm-12 col-md-10 col-lg-10" style="padding-top: 30px">

        <div class="panel-conam panel-primary-conam">
            <div class="panel-heading-conam"><strong>Riepilogo</strong></div>
            <div class="panel-body panel-body-background">
                <shared-alert [message]="messageTop" [type]="typeMessageTop" *ngIf="showMessageTop"></shared-alert>
                <shared-verbale-riepilogo></shared-verbale-riepilogo>

                <div class="row">
                    <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6 text-left">
                            <button type="button" class="btn-conam button-blue" (click)="CaricaVerbaleAudizione()"
                            *ngIf="creaAllegatoEnable">
                            <i class="glyphicon glyphicon-share-alt btn-glyphicon img-circle text-blue-conam"></i>
                                Carica verbale di audizione</button>
                    </div>
                    <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6 text-right">
                        <button type="button" class="btn-conam button-blue" (click)="goToCreaVerbaleAudizione()"
                            *ngIf="allegatoEnable">
                            <i class="glyphicon glyphicon-share-alt btn-glyphicon img-circle text-blue-conam"></i>Seleziona
                            soggetti
                            per verbale di audizione</button>
                    </div>
                </div>
                
            </div>
        </div>
    </div>
    <div class="principal-div col-xs-12 col-sm-12 col-md-1 col-lg-1"></div>
</div>