<div class="form-group row">
    <form>
        <img alt="Logo Regione Piemonte" src="assets/logo_RegPie.jpg" class="center">

        <div class="col-xs-3 col-sm-3col-md-3 col-lg-3"></div>
        <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6 padding-top-15" style="text-align: center">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                Direzione Risorse Finanziarie
            </div>
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                Settore Politiche Fiscali e Contenzioso Amministrativo
            </div>
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12" *ngIf="viewMailTributiField">
                {{datiTemplate.mailSettoreTributi}}
            </div>
        </div>
        <div class="col-xs-3 col-sm-3col-md-3 col-lg-3"></div>

        <div class="form-group col-xs-12 col-sm-12 col-md-12 col-lg-12 padding-top-25">
            <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6">
                Prot. n. {{datiTemplate.numeroProtocollo}}
            </div>
            <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6 text-right">
                Torino, {{dataOdierna}}
            </div>
        </div>

        <div class="form-group col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                CL. {{datiTemplate.classificazione}}
            </div>
        </div>

        <div class="form-group col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                F.N.
                <div class="form-control-view" style="display: inline-block">{{datiTemplate.fn}}
                </div>
                <!--<input type="text" id="f.n." class="form-control" style="display: inline-block; width: 20%" name="f.n."
                    [(ngModel)]="datiCompilati.fn" required *ngIf="!isAnteprima">-->
            </div>
        </div>
    </form>
</div>